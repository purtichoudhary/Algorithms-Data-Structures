class Solution {
    public String removeVowels(String S) {
        StringBuilder newString = new StringBuilder();
        for(char ch : S.toCharArray()) {
            if(ch == 'a' || ch == 'e' || ch == 'i' ||
               ch == 'o' || ch == 'u') {
                continue;
            }
            newString.append(ch);
        }
        return newString.toString();
    }
}
